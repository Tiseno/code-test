    "parcel-bundler": "^1.12.1",
    "postcss-modules": "^1.4.1",
    "react-hot-loader": "^4.6.5",
    "sass": "^1.17.0",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  },
