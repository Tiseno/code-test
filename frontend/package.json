{
  "name": "code-test-front-end",
  "version": "0.0.1",
  "description": "Stabelo code test front end application",
  "main": "index.tsx",
  "private": true,
  "scripts": {
    "clean": "rm -rf .cache .serve target",
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint": "tslint -p tsconfig.json -c tslint.json",
